{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    }
  },
  "emulators": {
    "dataconnect": {
      "dataDir": ".dataconnect/.dataconnect/pgliteData"
    }
  },
  "dataconnect": {
    "source": ".dataconnect"
  },
  "apphosting": {
    "backendId": "mtb-booking-app",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log"
    ]
  }
}
